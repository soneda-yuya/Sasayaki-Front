{
  "env": {
    "browser": true
  },
  "rules": {
    "max-len": [
      "off"
    ],
    "no-plusplus": [
      "off"
    ],
    "import/no-dynamic-require": [
      "off"
    ],
    "no-console": [
      "error",
      { "allow": ["info", "warn", "error"] }
    ],
    "prefer-destructuring": [
      "off"
    ]
  },
  "extends": "airbnb",
  "plugins": [
    "jsx-a11y",
    "import"
  ]
}
